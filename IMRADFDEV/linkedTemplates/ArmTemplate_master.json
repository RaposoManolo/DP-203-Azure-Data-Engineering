{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "IMRADFDEV"
		},
		"CashDataPrem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CashDataPrem'"
		},
		"CashDataTarget_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CashDataTarget'"
		},
		"APISource_ADLSgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'APISource_ADLSgen2'"
		},
		"AzureSql_ADFLogs_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "imrstockbondvalue.database.windows.net"
		},
		"AzureSql_ADFLogs_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "IMRStockBondsValue"
		},
		"CashDataPrem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\IMRaposo\\Documents\\Az203\\Data"
		},
		"CashDataPrem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "IMRaposo"
		},
		"TestLinkAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.alphavantage.co"
		},
		"APISource_ADLSgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://imrdatalakedev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "IMRADFDEV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CashDataTarget_connectionString": {
						"value": "[parameters('CashDataTarget_connectionString')]"
					},
					"APISource_ADLSgen2_accountKey": {
						"value": "[parameters('APISource_ADLSgen2_accountKey')]"
					},
					"CashDataPrem_password": {
						"value": "[parameters('CashDataPrem_password')]"
					},
					"AzureSql_ADFLogs_properties_typeProperties_server": {
						"value": "[parameters('AzureSql_ADFLogs_properties_typeProperties_server')]"
					},
					"AzureSql_ADFLogs_properties_typeProperties_database": {
						"value": "[parameters('AzureSql_ADFLogs_properties_typeProperties_database')]"
					},
					"TestLinkAPI_properties_typeProperties_url": {
						"value": "[parameters('TestLinkAPI_properties_typeProperties_url')]"
					},
					"APISource_ADLSgen2_properties_typeProperties_url": {
						"value": "[parameters('APISource_ADLSgen2_properties_typeProperties_url')]"
					},
					"CashDataPrem_properties_typeProperties_host": {
						"value": "[parameters('CashDataPrem_properties_typeProperties_host')]"
					},
					"CashDataPrem_properties_typeProperties_userId": {
						"value": "[parameters('CashDataPrem_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}