					{
						"name": "LogId",
						"type": "int",
						"precision": 10
					},
					{
						"name": "PipelineName",
						"type": "varchar"
					},
					{
						"name": "PipelineRunID",
						"type": "varchar"
					},
					{
						"name": "StartTime",
						"type": "datetime2",
						"scale": 0
					},
					{
						"name": "EndTime",
						"type": "datetime2",
						"scale": 0
					},
					{
						"name": "DurationSeconds",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Inserts",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Updates",
						"type": "int",
						"precision": 10
					},
					{
						"name": "Deletes",
						"type": "int",
						"precision": 10
					},
					{
						"name": "ErrorMessage",
						"type": "varchar"
					},
					{
						"name": "StatusMessage",
						"type": "varchar"
					}
