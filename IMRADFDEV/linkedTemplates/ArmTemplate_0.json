							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "pipelineReturnValue",
							"value": [
								{
									"key": "TestAlert1",
									"value": {
										"type": "Int",
										"content": []
									}
								}
