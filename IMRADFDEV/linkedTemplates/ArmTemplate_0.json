{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "IMRADFDEV"
		},
		"CashDataPrem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'CashDataPrem'"
		},
		"CashDataPrem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\IMRaposo\\Documents\\Az203\\Data"
		},
		"CashDataPrem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "IMRaposo"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/InvestmentCashFlow')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CashDataPrem')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "FileServer",
				"typeProperties": {
					"host": "[parameters('CashDataPrem_properties_typeProperties_host')]",
					"userId": "[parameters('CashDataPrem_properties_typeProperties_userId')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('CashDataPrem_password')]"
					}
				},
				"connectVia": {
					"referenceName": "InvestmentCashFlow",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/InvestmentCashFlow')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CashDataPrem')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "CashDataPrem",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "FileServerLocation",
						"fileName": "cashstatements_150924.csv"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Date",
						"type": "String"
					},
					{
						"name": "Description",
						"type": "String"
					},
					{
						"name": "Reference",
						"type": "String"
					},
					{
						"name": "Settlement date",
						"type": "String"
					},
					{
						"name": "Receipt (GBP)",
						"type": "String"
					},
					{
						"name": "Payment (GBP)",
						"type": "String"
					},
					{
						"name": "Balance (GBP)",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/CashDataPrem')]"
			]
		}
	]
}